    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:coverage": "jest --config jest.config.ts --coverage --watchAll=false",
    "test:coverage:excel": "npm run test:coverage && node -e \"const fs = require('fs'); const coverage = JSON.parse(fs.readFileSync('tests/coverage/coverage-summary.json', 'utf8')); let csv = 'File Path,Statements %,Lines %,Functions %,Branches %,Statements Covered,Statements Total,Lines Covered,Lines Total,Functions Covered,Functions Total,Branches Covered,Branches Total\\n'; for (const [filePath, data] of Object.entries(coverage)) { if (filePath === 'total') { csv += 'TOTAL PROJECT,' + data.statements.pct + ',' + data.lines.pct + ',' + data.functions.pct + ',' + data.branches.pct + ',' + data.statements.covered + ',' + data.statements.total + ',' + data.lines.covered + ',' + data.lines.total + ',' + data.functions.covered + ',' + data.functions.total + ',' + data.branches.covered + ',' + data.branches.total + '\\n'; } else { const fileName = filePath.split('/').pop(); csv += fileName + ',' + data.statements.pct + ',' + data.lines.pct + ',' + data.functions.pct + ',' + data.branches.pct + ',' + data.statements.covered + ',' + data.statements.total + ',' + data.lines.covered + ',' + data.lines.total + ',' + data.functions.covered + ',' + data.functions.total + ',' + data.branches.covered + ',' + data.branches.total + '\\n'; } } fs.writeFileSync('tests/coverage/test-coverage-report.csv', csv); console.log('âœ… Excel-compatible CSV report generated at tests/coverage/test-coverage-report.csv');\"",
    "test:e2e": "playwright test --config tests/configs/playwright.config.ts",
    "test:e2e:ui": "playwright test --config tests/configs/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config tests/configs/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config tests/configs/playwright.config.ts --debug"
  },
